{
  "name": "gemini-backend",
  "version": "1.0.0",
  "description": "Backend API for Gemini Software - Portfolio CRUD and Contact Management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "lint": "eslint . --ext .ts --max-warnings 10",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "build": "npm run lint && npm run type-check && prisma generate && tsc && npx tsc prisma/seed.ts --outDir dist/prisma --esModuleInterop --resolveJsonModule --skipLibCheck",
    "build:prod": "npm run lint && npm run type-check && prisma generate && tsc && npx tsc prisma/seed.ts --outDir dist/prisma --esModuleInterop --resolveJsonModule --skipLibCheck",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "script": "node scripts.js",
    "script:help": "node scripts.js help",
    "script:optimize": "node scripts.js optimize-images",
    "script:update-db": "node scripts.js update-db-images",
    "script:check": "node scripts.js check-projects"
  },
  "dependencies": {
    "@flydotio/litestream": "^1.0.1",
    "@prisma/client": "^5.15.0",
    "@types/compression": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "prisma": "^5.15.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.5.2"
  },
  "prisma": {
    "seed": "node dist/prisma/seed.js"
  },
  "dockerfile": {
    "litestream": true
  }
}
